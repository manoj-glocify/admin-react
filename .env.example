# API Configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_AUTH_TOKEN_KEY=auth_token
REACT_APP_AUTH_REFRESH_TOKEN_KEY=refresh_token
REACT_APP_AUTH_TOKEN_EXPIRY=3600

# Application
REACT_APP_NAME=Admin Dashboard
REACT_APP_VERSION=1.0.0
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_2FA=true
REACT_APP_ENABLE_GOOGLE_AUTH=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# External Services
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_SENTRY_DSN=your_sentry_dsn
